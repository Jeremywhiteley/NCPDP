  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  AdministrationTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="AdministrationTiming">
    <xsd:sequence>
      <xsd:sequence minOccurs="0">
        <xsd:element name="AdministrationTimingNumericValue" type="datatypes:n"/>
        <xsd:element name="VariableAdministrationTimingModifier" type="datatypes:ANDORTO"/>
        <xsd:sequence minOccurs="0">
          <xsd:element name="AdministrationTimingNumericValue" type="datatypes:n"/>
        </xsd:sequence>
        <xsd:element name="AdministrationTimingUnits" type="datatypes:SigCode"/>
      </xsd:sequence>
      <xsd:element minOccurs="0" name="AdministrationTimingModifier" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as before, during, with, without, after, while, at., on, start, end.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AdministrationTimingEvent" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="AdministrationTimingClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Allergy  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Allergy">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="NoKnownAllergies" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>If the sender sends “Y” (Yes), no other fields are sent in this element.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SourceOfInformation" type="ecl:SourceOfInformationCode">
        <xsd:annotation>
          <xsd:documentation>Sender indicates where the sender received the allergy information if known. 
The source of information is the ultimate source. The values are the patient or patient representative, or observed by the clinician or provider.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular allergy or intolerance was known to be active for the patient and when it was no longer active – if known.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular allergy or intolerance was known to be active for the patient and when it was no longer active – if known.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AdverseEvent" type="datatypes:SNOMEDAdverseEventCode">
        <xsd:annotation>
          <xsd:documentation>Value: LD. Describes the type of product and intolerance suffered by the patient. The type of product shall be classified with respect to whether the adverse event occurs in relationship with a medication, food, or environmental or other product. The adverse event should also be classified more specifically as an allergy, non-allergy intolerance, or just adverse reaction if that level of detail is not known.
Required No Known Allergies = “N” (No), meaning allergies are known and will be reported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DrugProductCoded" type="datatypes:DrugProductCodeWithText">
        <xsd:annotation>
          <xsd:documentation>Identifies the product to which the patient is allergic.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ReactionCoded" type="datatypes:SNOMEDCode">
        <xsd:annotation>
          <xsd:documentation>The type of reaction the patient has to the allergy. Value: LD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SeverityCoded" type="datatypes:SNOMEDCode">
        <xsd:annotation>
          <xsd:documentation>Identifies the severity of the reaction the patient has to the allergy. Value: LD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Attachment  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Attachment">
    <xsd:sequence>
      <xsd:element name="AttachmentSource" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>The Source of the Info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AttachmentData" type="xsd:base64Binary">
        <xsd:annotation>
          <xsd:documentation>all attached information is Base64 encoded</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:group ref="structures:ClinicalInformation"/>
      <xsd:element name="MIMEType" type="ecl:MIMETypeCode">
        <xsd:annotation>
          <xsd:documentation>Defines the content nature of the attachmentData.  Internet standard defined in RFC 2045, RFC 2046, RFC 2047, RFC 4288, RFC 4289 and RFC 2049. See ECL for currently recognized and enumerated MIME Types for purposes in NCPDP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  AttachmentWithControlNumber  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="AttachmentWithControlNumber">
    <xsd:annotation>
      <xsd:documentation>For electronic prior authorizations transactions, Attachments are supported both to answer or clarify a specific question, or more than one question. If an attachment is sent to address a specific question, then the CodedReferenceAnswer Attachment element is used.  If an attachment will address multiple questions, then SeeTransactionLevelAttachmentControlNumber indicates an attachment at the transaction level should be viewed.  For example, if one clinical document can be used to answer multiple questions, the document can be sent once and referred to for the multiple answers.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="AttachmentControlNumber" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Indicates the control number of the attachment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AttachmentSource" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>The Source of the Info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AttachmentData" type="xsd:base64Binary">
        <xsd:annotation>
          <xsd:documentation>All attached information is Base64 encoded</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:group minOccurs="0" ref="structures:ClinicalInformation"/>
      <xsd:element name="MIMEType" type="ecl:MIMETypeCode">
        <xsd:annotation>
          <xsd:documentation>Defines the content nature of the attachmentData.  Internet standard defined in RFC 2045, RFC 2046, RFC 2047, RFC 4288, RFC 4289 and RFC 2049. See ECL for currently recognized and enumerated MIME Types for purposes in NCPDP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Attachments  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Attachments">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Attachment" type="structures:Attachment"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  BaseBenefitsCoordination  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="BaseBenefitsCoordination">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PayerIdentification" type="datatypes:PayerID">
        <xsd:annotation>
          <xsd:documentation>For Census - for Private Pay, this will not be sent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PayerName" type="datatypes:an1..70">
        <xsd:annotation>
          <xsd:documentation>Census - if Private Pay – This is the responsible Party Name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CardholderID" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="CardHolderName" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="GroupID" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="PayerResponsibilityCode" type="ecl:PayerResponsibilityCode"/>
      <xsd:element minOccurs="0" name="PatientRelationship" type="ecl:PatientRelationshipCode"/>
      <xsd:element minOccurs="0" name="PersonCode" type="datatypes:n1..3"/>
      <xsd:element minOccurs="0" name="GroupName" type="datatypes:an1..70"/>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
        <xsd:annotation>
          <xsd:documentation>For Census - Responsible Party’s address</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
        <xsd:annotation>
          <xsd:documentation>For RxChangeRequest: The payer-based telephone number exchanged between the pharmacy and the prescriber is the prior authorization telephone number at the payer that the pharmacy may have knowledge of (rather than the payer’s phone number for member questions or billing questions etc at the payer).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Consent" type="ecl:ConsentCode">
        <xsd:annotation>
          <xsd:documentation>Note: If Patient Consent = N, it is expected the RxHistory transactions would not be sent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PBMMemberID" type="datatypes:an1..80">
        <xsd:annotation>
          <xsd:documentation>Payer assigned Unique Member ID.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ResponsibleParty" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>For Census, Full Name of Responsible Party.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ServiceTypeCoded" type="ecl:ServiceTypeCode">
        <xsd:annotation>
          <xsd:documentation>Medication list contains all current medication orders as of the current date and time of the response, for the patient indicated. Current status is determined by the point of care responder. “Current” is medication orders which have not been discontinued.

Value “C” is “Not used” to request medication history from pharmacy or payer/processor. 

RxHistoryRequest:  When this field contains a value of “C” in the Medication History request message, Date is not sent. 

RxHistoryResponse: The response echoes back the “C” value in this field.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
