  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Sig  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Sig">
    <xsd:sequence>
      <xsd:element name="SigText" type="datatypes:an1..1000">
        <xsd:annotation>
          <xsd:documentation>SigText contains either 1) completely free text with no corresponding structured content OR 2) This is a generated structured English version of the codified sig content that follows the sig grammar in the XML. The SigText should have no more and no less then what is in the structured Sig content. If you can not directly generate the SigText from the Structured Sig, do not send the structured Sig. If the structured Sig exists, the SigText must be generated directly from Structured Sig. If this is not possible for whatever reason, do not send the structured Sig.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Structured Sig is the preferred but if for some reason you can not send the structured Sig, use the SigText.</xsd:documentation>
        </xsd:annotation>
        <xsd:element name="CodeSystem">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="SNOMEDVersion" type="datatypes:an"/>
              <xsd:element name="FMTVersion" type="datatypes:an">
                <xsd:annotation>
                  <xsd:documentation>If using structured Sig but not needing to send any of the elements that use dose form, send a valid FMT version using by the application for other sigs.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Instruction" type="structures:Instruction">
          <xsd:annotation>
            <xsd:documentation>Instruction is the focal point of the Structured Sig - An Instruction is defined as Dose Administration with one to many associated Timing and/or Durations</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:sequence maxOccurs="49" minOccurs="0">
          <xsd:element name="MultipleInstructionModifier" type="datatypes:ANDORTHENNOT"/>
          <xsd:element name="Instruction" type="structures:Instruction"/>
        </xsd:sequence>
        <xsd:sequence maxOccurs="50" minOccurs="0">
          <xsd:element name="IndicationForUse" type="structures:Indication"/>
        </xsd:sequence>
        <xsd:element maxOccurs="50" minOccurs="0" name="MaximumDoseRestriction" type="structures:MaximumDoseRestriction"/>
        <xsd:element minOccurs="0" name="ClarifyingFreeText" type="datatypes:an1..255">
          <xsd:annotation>
            <xsd:documentation>See SCRIPT Implementation guide for proper use of clarifying free text elements and code sets</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SpecializedMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SpecializedMedication">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DrugDescription" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Is the self-contained full drug name, strength, and form. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Quantity" type="datatypes:Quantity">
        <xsd:annotation>
          <xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 85.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LastFillDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value LD.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 36.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 07.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value  PE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DateValidated" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Substitutions" type="ecl:SubstitutionCode">
        <xsd:annotation>
          <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagnosis" type="structures:Diagnosis"/>
      <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note" type="datatypes:an1..210">
        <xsd:annotation>
          <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
        <xsd:annotation>
          <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
        <xsd:annotation>
          <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Sig" type="structures:Sig">
        <xsd:annotation>
          <xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Compound" type="datatypes:CompoundIngredient">
        <xsd:annotation>
          <xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SoldDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AdditionalRefillsAuthorized" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value A. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
        <xsd:annotation>
          <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
        <xsd:annotation>
          <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
        <xsd:annotation>
          <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
        <xsd:annotation>
          <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriber"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistorySource" type="datatypes:HistorySource"/>
      <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
        <xsd:annotation>
          <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
        <xsd:annotation>
          <xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Name of the manufacturer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Supervisor  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Supervisor">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. 

Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorOptional">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:SupervisorID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorMandatoryAddress  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorMandatoryAddress">
    <xsd:annotation>
      <xsd:documentation>Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:Supervisor">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:SupervisorID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="datatypes:MandatoryAddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorOptional  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorOptional">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:SupervisorID">
        <xsd:annotation>
          <xsd:documentation>On the mandatory transactions: One occurrence is the prescriber number. One occurrence may be used to identify the clinic. One occurrence may be used to identify a supervisor. At least one occurrence must contain “HPI” (NPI). Prescriber individual (not organizational) NPI Number is mandatory. This is mandatory for the Prescriber (PC), not the Supervisor (SU). For all transactions except RxHistoryResponse, if the prescriber has an NPI, one occurrence must contain the value “HPI” (NPI). For all transactions except RxHistoryResponse, if the prescriber has a DEA number, one occurrence must contain the value “DH” (DEA Number).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10"/>
      <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an1..70"/>
      <xsd:element name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  TimingAndDuration  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="TimingAndDuration">
    <xsd:choice>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element name="AdministrationTiming" type="structures:AdministrationTiming">
            <xsd:annotation>
              <xsd:documentation>Defines a specific administration day, date or time, or event. Examples such as Breakfast, lunch, dinner, sleep, appointment, procedure, surgery, exercise, sex, school, travel, cycle (HRT, PMS, etc.), completion of other medication regimen. Empty stomach.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Frequency" type="structures:FrequencyTiming">
            <xsd:annotation>
              <xsd:documentation>Defines a frequency of administration. Frequency is events per unit of time.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Interval" type="structures:IntervalTiming"/>
          <xsd:element name="RateOfAdministration" type="structures:RateOfAdministration">
            <xsd:annotation>
              <xsd:documentation>Primary use will be in non-retail settings, i.e. hospital, acute care, long term care, home care. May be used in conjunction with Dose Calculation .</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
        <xsd:element minOccurs="0" name="TimingClarifyingFreeText" type="datatypes:an1..255"/>
      </xsd:sequence>
      <xsd:element name="Duration" type="structures:Duration"/>
      <xsd:element name="DurationTrigger" type="structures:DurationTriggerTiming">
        <xsd:annotation>
          <xsd:documentation>The event that indicates the completion of the duration of use or reason to stop. Examples such as until gone, until symptoms relieved, unless symptoms worsen, until return from trip, unless new symptoms appear, if lightheaded, if side affects appear.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="StopIndicator" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>The event that indicates the completion of the duration of use or reason to stop. Examples such as then stop, then discontinue.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Vehicle  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Vehicle">
    <xsd:sequence>
      <xsd:element name="VehiclePreposition" type="datatypes:SigCode"/>
      <xsd:element name="VehicleQuantity" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="MultipleVehicleModifier" type="datatypes:ANDORTO"/>
        <xsd:element name="VehicleQuantity" type="datatypes:n"/>
      </xsd:sequence>
      <xsd:element name="VehicleUnitOfMeasure" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as Milliliter, ounce.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Vehicle" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="VehicleClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:annotation>
    <xsd:documentation>The NCPDP structures.xsd defines common and reusable domain structures typically composed of datatypes and potentially other structures.</xsd:documentation>
  </xsd:annotation>
</xsd:schema>