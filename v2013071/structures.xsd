          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
            <xsd:annotation>
