              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CardholderID" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CardHolderName" type="datatypes:NameType">
